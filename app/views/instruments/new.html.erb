<div class="card-half">
<h2>Create a new music instrument</h2><br>
  <%= simple_form_for(@instrument) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :photo, as: :file %>
    <%= f.submit class: 'btn-large' %>
  <% end %>
</div>
